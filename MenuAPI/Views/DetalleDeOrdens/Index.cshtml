@model IEnumerable<MenuAPI.DetalleDeOrden>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CantidadOrden)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NotaDetalleOrden)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstadoDetalleOrden)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Menu.CodigoMenu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Orden.CodigoOrden)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CantidadOrden)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NotaDetalleOrden)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstadoDetalleOrden)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Menu.CodigoMenu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Orden.CodigoOrden)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

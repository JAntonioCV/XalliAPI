@model IEnumerable<MenuAPI.Orden>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CodigoOrden)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaOrden)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TiempoOrden)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstadoOrden)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoOrden)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaOrden)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TiempoOrden)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstadoOrden)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

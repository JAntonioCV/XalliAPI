@model IEnumerable<MenuAPI.CategoriaMenu>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CodigoTipoPlatillo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DescripcionTipoPlatillo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstadoTipoPlatillo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoTipoPlatillo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DescripcionTipoPlatillo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstadoTipoPlatillo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
